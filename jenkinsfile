pipeline {
    agent any

    parameters {
        booleanParam(defaultValue: true, description: '', name: 'userFlag')
        string(defaultValue: MARVEL, description: '', name: 'userFlag')
        choice(choices: 'IronMan\nHulk\nSpider-Man\nThor', description: '', name: 'userFlag')
    }

    stages {
        stage("foo") {
            steps {
                echo "flag: ${params.userFlag}"
            }
        }
    }
}
